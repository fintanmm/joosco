{
    "name": "fintanmm/joosco",
    "type": "library",
    "description": "JOOSCO is a front end for CMIS using the Joomla framework",
    "homepage": "https://github.com/fintanmm/joosco",
    "license": "GPLv2",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/fintanmm/joosco/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/fintanmm/joosco/issues"
    },
    "repositories": [
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/joomla/joomla-cms/archive/staging.zip"
                },
                "name": "joomla/cms",
                "version": "3.4.1"
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/waddedMeat/phing-vagrant.git"
        }
    ],    
    "require": {
        "php": ">=5.3.0",
        "dkd/php-cmis": "^0.1.0"
    },
    "require-dev": {
        "phing/phing": "^2.13",
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "joomla/cms": "3.4.1",        
        "pdepend/pdepend": "~1.0",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~1.0",
        "phpunit/dbunit": "1.4.0",
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.0",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Joosco\\": "src/",
            "Joosco\\Component\\": "src/Component",
            "Joosco\\Plugin\\": "src/Plugin"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/joomla/cms/tests/unit/bootstrap.php",
            "vendor/joomla/cms/tests/unit/suites/libraries/joomla/view/mocks/JModelMock.php",
            "vendor/joomla/cms/libraries/joomla/user/authentication.php"
        ],        
        "psr-4": {
            "Joosco\\UnitTests\\": "tests/unit/"
        }
    }
}
